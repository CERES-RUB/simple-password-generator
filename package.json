{
    "name": "simpel-password-generator",
    "author": "Leonhard KÃ¼per",
    "type": "module",
    "version": "1.0.0",
    "private": true,
    "license": "CC0-1.0",
    "description": "rollup + typescript + eslint + postcss",
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25",
        "@rollup/plugin-node-resolve": "^15",
        "@rollup/plugin-terser": "^0.4.1",
        "@rollup/plugin-typescript": "^11",
        "@types/node": "^20",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "browser-sync": "^3",
        "concurrently": "^8",
        "copy-and-watch": "^0",
        "cssnano": "^6",
        "eslint": "^8.57.0",
        "import-http": "^0",
        "postcss": "^8",
        "postcss-cli": "^11",
        "postcss-import": "^16",
        "postcss-import-url": "^7",
        "postcss-preset-env": "^9",
        "postcss-url": "^10",
        "rimraf": "^5",
        "rollup": "^4",
        "rollup-plugin-multi-input": "^1",
        "ts-node": "^10",
        "tsconfig-paths": "^4",
        "tslib": "^2",
        "typescript": "^5.4.2",
        "typescript-eslint": "^7.3.1"
    },
    "scripts": {
        "dev:pre": "rimraf dev_build && mkdir dev_build",
        "dev:copy": "copy-and-watch --watch 'src/**/*.{html,webp}' 'dev_build/'",
        "dev:build:js": "rollup --config --watch",
        "dev:build:css": "postcss 'src/**/*.css' --verbose --watch --base src --dir dev_build",
        "dev:server": "browser-sync --port 8080 dev_build/ --files 'dev_build/**/*' --no-open --no-notify --watch",
        "dev": "yarn dev:pre && concurrently --kill-others 'yarn:dev:copy' 'yarn:dev:build:*' 'yarn:dev:server'",
        "prod:build:js": "rollup --config --environment NODE_ENV:production",
        "prod:build:css": "postcss src/css/index.css --base src --dir prod_build --env production",
        "prod:copy": "copy-and-watch 'src/**/*.{html,webp,woff2}' 'prod_build/'",
        "prod:build": "yarn && concurrently 'yarn:prod:build:*' && yarn 'prod:copy'",
        "prod": "yarn prod:build",
        "lint": "eslint src/**/*.ts"
    },
    "packageManager": "yarn@4.1.1+sha256.f3cc0eda8e5560e529c7147565b30faa43b4e472d90e8634d7134a37c7f59781"
}
